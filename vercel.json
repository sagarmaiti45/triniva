{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/plans",
      "dest": "/public/plans.html"
    },
    {
      "src": "/privacy-policy",
      "dest": "/public/privacy-policy.html"
    },
    {
      "src": "/terms-conditions",
      "dest": "/public/terms-conditions.html"
    },
    {
      "src": "/refund-policy",
      "dest": "/public/refund-policy.html"
    },
    {
      "src": "/about-us",
      "dest": "/public/about-us.html"
    },
    {
      "src": "/contact-us",
      "dest": "/public/contact-us.html"
    },
    {
      "src": "/important-info",
      "dest": "/public/important-info.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/auth/login$",
      "dest": "/public/auth/login.html"
    },
    {
      "src": "/auth/signup$",
      "dest": "/public/auth/signup.html"
    },
    {
      "src": "/auth/callback$",
      "dest": "/public/auth/callback.html"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/public/auth/$1"
    },
    {
      "src": "/favicon.png",
      "dest": "/public/favicon.png"
    },
    {
      "src": "/demo.html",
      "dest": "/public/demo.html"
    },
    {
      "src": "/chat/(.*)",
      "dest": "/public/index.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["bom1"]
}