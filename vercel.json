{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/plans",
      "dest": "/server.js"
    },
    {
      "src": "/privacy-policy",
      "dest": "/server.js"
    },
    {
      "src": "/terms-conditions",
      "dest": "/server.js"
    },
    {
      "src": "/refund-policy",
      "dest": "/server.js"
    },
    {
      "src": "/about-us",
      "dest": "/server.js"
    },
    {
      "src": "/contact-us",
      "dest": "/server.js"
    },
    {
      "src": "/important-info",
      "dest": "/server.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/public/auth/$1"
    },
    {
      "src": "/favicon.png",
      "dest": "/public/favicon.png"
    },
    {
      "src": "/demo.html",
      "dest": "/public/demo.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["bom1"]
}